    {
        "id": "fn-ahu",
        "type": "function",
        "z": "2fe43d6eec7777e1",
        "name": "Generate AHU Telemetry",
        "func": "const ahus = [\n  'AHU-01', 'AHU-02', 'AHU-03', 'AHU-04', 'AHU-05',\n  'AHU-06', 'AHU-07', 'AHU-08', 'AHU-09', 'AHU-10', 'AHU-11'\n];\n\nconst messages = [];\n\nahus.forEach(ahu => {\n  const temperature = +(20 + Math.random() * 12).toFixed(1);\n  const humidity = +(40 + Math.random() * 30).toFixed(1);\n  const fan_status = Math.random() > 0.15 ? 'ON' : 'OFF';\n\n  let status = 'OK';\n  if (fan_status === 'OFF') {\n    status = 'ALARM';\n  } else if (temperature < 20 || temperature > 28 || humidity > 65) {\n    status = 'WARNING';\n  }\n\n  const timestamp = new Date().toISOString();\n\n  messages.push({\n    topic: `hvac/planta1/${ahu}`,\n    payload: {\n      plantId: 'planta1',\n      stationId: ahu,\n      timestamp,\n      points: {\n        temperature: {\n          value: temperature,\n          unit: '°C',\n          quality: status === 'OK' ? 'GOOD' : status === 'ALARM' ? 'BAD' : 'UNCERTAIN'\n        },\n        humidity: {\n          value: humidity,\n          unit: '%',\n          quality: status === 'OK' ? 'GOOD' : status === 'ALARM' ? 'BAD' : 'UNCERTAIN'\n        },\n        fan_status: {\n          value: fan_status,\n          unit: '',\n          quality: status === 'OK' ? 'GOOD' : status === 'ALARM' ? 'BAD' : 'UNCERTAIN'\n        },\n        status: {\n          value: status,\n          unit: '',\n          quality: status === 'OK' ? 'GOOD' : status === 'ALARM' ? 'BAD' : 'UNCERTAIN'\n        }\n      }\n    }\n  });\n});\n\nreturn [messages];\n",
        "outputs": 1,
